image_from="izdock/httpd:php5.6"

source <(grep -e "^PHP_VERSION" ../httpd/vars.php56)

tag_ver_major=${PHP_VERSION_MAJOR}
tag_ver_minor=${PHP_VERSION_MINOR}
tag_ver_patch=${PHP_VERSION_PATCH}
tag_ver=${PHP_VERSION_MAJOR}.${PHP_VERSION_MINOR}.${PHP_VERSION_PATCH}

tag_latest=no
tags="${tag_ver_major} ${tag_ver_major}.${tag_ver_minor} ${tag_ver} ${tag_ver}-${tag_ver_build} "

source $PWD/vars
